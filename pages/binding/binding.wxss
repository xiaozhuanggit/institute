.key-value{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.key image{
  width: 34rpx;
  height: 34rpx;
  vertical-align: middle;
  margin: -4rpx 20rpx 0 0;
}
.value{
  flex:1;
  text-align: right;
  color: #C0A45B;
  font-size: 26rpx;
}
.key-value input[type="text"]{
  text-align: left;
  color: #333;
}
.right-icon{
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
  margin: 0 0 0 10rpx; 
}
button::after{
  border: none;
}
.code-btn{
  background-color: transparent;
  border: none;
  outline: none;
  font-size: 30rpx;
  color: #C0A45B;
}
.code-btn[disabled]{  
    background-color: transparent !important;
} 

.btn{
  width: 100%;
  position: fixed;
  bottom: 40rpx;
  left: 0;
  
}
.btn button{
  width: 350rpx;
  height: 78rpx;
  line-height: 78rpx;
  background: #C0A45B;
  border-radius: 40rpx;
  font-size: 30rpx;
  color: #fff;
}
