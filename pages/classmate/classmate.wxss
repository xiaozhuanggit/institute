/* pages/classmate/classmate.wxss */
@import "/template/template.wxss";
.mate-list{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 120rpx;
  font-size: 26rpx;
  color: #999;
}
.mate-head{
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  overflow: hidden;
}
.mate-head image{
  width: 100%;
  height: 100%;
}
.nickname{
  flex: 1;
  font-size: 30rpx;
  color: #333;
}
